 <!DOCTYPE html>
 <html lang="en">
 <head>
     <meta charset="UTF-8">
     <meta http-equiv="X-UA-Compatible" content="IE=edge">
     <link rel="stylesheet" href="/materialdesignicons.min.css">
     <link rel="stylesheet" href="/vendor.bundle.base.css">
     <link rel="stylesheet" href="/style1.css">
     <link rel="shortcut icon" href="/favicon.ico"/>
     <meta name="viewport" content="width=device-width, initial-scale=1.0">
     <title>Proprietors
     </title>
 </head>
 <body>

     <!--NAV-->

     <%- include('./partials/prop/nav.ejs') %>


     <!--Sidebar-->

     <%- include('./partials/prop/sidebar.ejs') %>


     <div class="row">
      <div class="col-lg-6 grid-margin stretch-card">
        <div class="card">
          <div class="card-body">
            </p>
            <table class="table">
              <thead>
                <tr>
                  <td>Student Name</td>
                  <td>Payment Status</td>
                  <td>Price</td>
                </tr>
              </thead>
              <tbody>

     <% if(applications.length>0) {%>
      <% applications.forEach(prop=>{ %> 
     
    
       <tr>
                      <td><%=prop.studentName%></td>
                      <td><%=prop.paymentStatus%></td>
                      <td><%=prop.price%></td>
                      <td>
                        <form action="/addtenant" method="post">
                          <input type="hidden" name="id" value="<%= prop.id %>">
                          <button class="btn btn-block btn-gradient-primary btn-lg font-weight-medium auth-form-btn">Approve</button>

                        </form>
                      </td>
  
                      <td> 
                        <form action="/deny" method="post">
                          <input type="hidden" name="appid" value="<%= prop.id %>">
                          <button class="btn btn-block btn-gradient-primary btn-lg font-weight-medium auth-form-btn">Deny</button>
                        </form>
                      </td>
  
                    
                    </tr>



           <% })%>
 
           <%}%>

           
          </tbody>
        </table>
        <br>
        <label class="badge badge-success"><%= Message %></label>                   
        <div class="mt-3">             
         
          
      </div>
      </div>
    </div>
   
  </div>

           

     
     
     
 </body>
  <!--JS SCRIPTS-->

  <script src="/vendor.bundle.base.js"></script>
  <script src="/Chart.min.js"></script>
  <script src="/jquery.cookie.js" type="text/javascript"></script>
  <script src="/off-canvas.js"></script>
  <script src="/hoverable-collapse.js"></script>
  <script src="/misc.js"></script>
  <script src="/dashboard.js"></script>
  <script src="/todolist.js"></script>
 </html>