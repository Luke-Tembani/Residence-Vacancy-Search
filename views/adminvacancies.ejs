<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <link rel="stylesheet" href="/materialdesignicons.min.css">
    <link rel="stylesheet" href="/vendor.bundle.base.css">
    <link rel="stylesheet" href="/style1.css">
    <link rel="shortcut icon" href="/favicon.ico"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Vacancies</title>
</head>
<body>

    <!--NAV-->

    <%- include('./partials/nav.ejs') %>


    <!--Sidebar-->

    <%- include('./partials/sidebar.ejs') %>

    <!--Proprietors Table-->
    
    <div class="card">
        <div class="card-body">
          <h4 class="card-title">Vacancies</h4>
          <table class="table table-hover">
            <thead>
              <tr>
                <td>Proprietor</td>
                <td>Address</td>
                <td>No. of Students</td>
                <td>Gender</td>
                <td>Price</td>
                <td>Wifi</td>
                <td>Geyser</td>
                <td>Solar</td>
                <td>Fridge</td>
                <td>Gas</td>

              </tr>
           
              
            </thead>
            
            <tbody>
              
           
              <% if(propvacs.length>0) {%>
                <% propvacs.forEach(vac=>{%> 

                    <tr>
                        <td><%=vac.proprietor%></td>
                        <td><%=vac.address%></td>
                        <td><%=vac.number%></td>
                        <td><%=vac.gender%></td>
                        <td><%=vac.price%></td>
                        <td><%=vac.wifi%></td>
                        <td><%=vac.geyser%></td>
                        <td><%=vac.solar%></td>
                        <td><%=vac.fridge%></td>
                        <td><%=vac.gas%></td>
                    </tr>                

               <% })%>

               <%}%>
              
            </tbody>
           
          </table>
          <h2 style="margin-left: 20px;color:red;"><%= Message %></h2>
        </div>
      </div>
    
    
    
</body>
 <!--JS SCRIPTS-->

 <script src="/vendor.bundle.base.js"></script>
 <script src="/Chart.min.js"></script>
 <script src="/jquery.cookie.js" type="text/javascript"></script>
 <script src="/off-canvas.js"></script>
 <script src="/hoverable-collapse.js"></script>
 <script src="/misc.js"></script>
 <script src="/dashboard.js"></script>
 <script src="/todolist.js"></script>
</html>